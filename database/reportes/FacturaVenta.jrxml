	<queryString language="SQL">
		<![CDATA[SELECT v.id, v.fecha_venta, pv.id_producto AS nro_producto, 
				p.nombre_producto, p.precio_producto, pv.unidades_vendidas_prod AS unidades,
				(pv.unidades_vendidas_prod * p.precio_producto) AS total, v.nro_pago, v.monto_final_venta
			FROM ventas v
			INNER JOIN productos_vendidos pv ON v.id = pv.id_venta
			INNER JOIN productos p ON p.id = pv.id_producto
			WHERE v.id = $P{NroVenta};]]>
	</queryString>
